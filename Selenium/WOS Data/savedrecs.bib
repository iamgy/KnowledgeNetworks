@article{ ISI:000166388400006,
Author = {Giordano, A and Nisoli, E and Tonello, C and Cancello, R and Carruba, MO
   and Cinti, S},
Title = {{Expression and distribution of heme oxygenase-1 and-2 in rat brown
   adipose tissue: The modulatory role of the noradrenergic system}},
Journal = {{FEBS LETTERS}},
Year = {{2000}},
Volume = {{487}},
Number = {{2}},
Pages = {{171-175}},
Month = {{DEC 29}},
Abstract = {{To investigate whether brown adipose tissue (BAT) expresses the
   inducible (HO-1) and the constitutive (HO-2) isoform of heme oxygenase,
   reverse transcriptase-polymerase chain reaction, Western blotting and
   immunohistochemistry were performed on interscapular BAT (IBAT) from
   rats acclimated at environmental temperature or exposed to cold. Both HO
   isoforms were detected in rat IBAT. They were immunolocalized in the
   cytoplasm and/or nuclei of brown adipocytes, in parenchymal capillaries,
   arteries and in some veins and nerves. Whereas cold exposure did not
   affect HO-2 expression, it significantly increased the expression of
   HO-1, both at mRNA (about 3-fold) and protein (about 2-fold) levels,
   reflecting the increased expression of HO-1 in the brown adipotytes and
   endothelial cells of parenchymal capillaries. Western blotting of
   cytosolic and nuclear protein extracts from cultured differentiated
   brown adipocytes showed that HO-1 and HO-2 are indeed localized in the
   cytosol and nuclei of brown adipocytes; and that noradrenaline
   stimulation significantly increased their amount in cytosol but not in
   the nuclear fraction. (C) 2000 Federation of European Biochemical
   Societies. Published by Elsevier Science B.V. All rights reserved.}},
