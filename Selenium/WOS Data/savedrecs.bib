@article{ ISI:000166114600018,
Author = {Wei, LN and Hu, XL and Chandra, D and Seto, E and Farooqui, M},
Title = {{Receptor-interacting protein 140 directly recruits histone deacetylases
   for gene silencing}},
Journal = {{JOURNAL OF BIOLOGICAL CHEMISTRY}},
Year = {{2000}},
Volume = {{275}},
Number = {{52}},
Pages = {{40782-40787}},
Month = {{DEC 29}},
Abstract = {{Receptor-interacting protein 140 (RIP140) encodes a histone deacetylase
   (HDAC) inhibitor-sensitive repressive activity. Direct interaction of
   RIP140 with HDAC1 and HDAC3 occurs in vitro and in vivo as demonstrated
   in co-immunoprecipitation and glutathione S-transferase pull-down
   experiments. The HDAC-interacting domain of RIP140 is mapped to its
   N-terminal domain, between amino acids 78 and 303 based upon glutathione
   S-transferase pull-down experiments. In chromatin immunoprecipitation
   assays, it is demonstrated that histone deacetylation occurs at the
   chromatin region of the Ga14 binding sites as a result of Ga14 DNA
   binding domain-tethered RIP expression. The immunocomplexes of RIP140
   from cells transfected with RIP140 and HDAC are able to deacetylate
   histone proteins in vitro. This study presents the first evidence for
   RIP140 as a negative coregulator for nuclear receptor actions by
   directly recruiting histone deacetylases and categorizes RIP140 as a
   novel negative coregulator that is able to directly interact with HDACs.}},
